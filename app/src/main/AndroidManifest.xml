            android:name=".ui.activity.search.SearchFocusActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.checkout.ActivityCheckout"
            android:exported="false" />
        <activity
            android:name=".ui.activity.success.ActivitySuccess"
            android:exported="false" />
        <activity
            android:name=".ui.activity.profile.edit.EditAccountActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.detail.ProductDetailActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.launch.SecondSlashActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.launch.SlashActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activity.changepass.SetPassword"
            android:exported="false" />
        <activity
            android:name=".ui.activity.changepass.ForgotPasswordActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.changepass.OTP_confirm"
            android:exported="false" />
        <activity
